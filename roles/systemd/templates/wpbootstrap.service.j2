[Unit]
Description=wpbootstrap
After=network-online.target cloud-config.service rc-local.service

[Service]
Type=simple
ExecStartPre=/usr/bin/cloud-init modules --mode=final (code=exited, status=0/SUCCESS)
ExecStart=/usr/bin/nohup /usr/bin/wpbootstrap.sh
PIDFile=/var/run/wpbootstrap.pid

[Install]
WantedBy=multi-user.target
